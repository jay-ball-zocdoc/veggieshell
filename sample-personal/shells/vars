# your favourite term: eterm, xterm, rxvt, etc. include any options
myxterm="rxvt -fg grey90 -bg grey10"

# for some machines, DNS gets funky
if [ "$DISPLAY" = "kerplunk:0.0" ]; then
	export DISPLAY=$fromfqdn:0
fi
